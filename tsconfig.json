{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./dist/",
      "removeComments": true,
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "preserveConstEnums": true
  },
  "include": [
      "./src/**/*"
  ]
}